{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "ReverseProxy": {
        "Routes": {
            "ProductRoute": {
                "ClusterId": "ProductCluster",
                "Match": {
                    "Path": "/api/Products/{**catch-all}"
                }                
            }
        },
        "Clusters": {
            "ProductCluster": {
                "Destinations": {
                  "Destination1": {
                    "Address": "http://demowebapi-srv-product"
                  }
                }
            }
        }
    },
    "AllowedHosts": "*"
}
